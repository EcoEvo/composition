#^ @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
#^ @prefix rbmo: <http://purl.org/rbm/rbmo#>.
#^ @prefix sbol: <http://sbols.org/v2#>.
#^ @prefix so: <http://identifiers.org/so/>.
#^ @prefix go: <http://identifiers.org/go/>.
#^ @prefix pr: <http://identifiers.org/pr/>.
#^ @prefix sbo: <http://identifiers.org/biomodels.sbo/>.
#^ @prefix dct: <http://purl.org/dc/terms/>.
#^ @prefix owl: <http://www.w3.org/2002/07/owl#>.
#^ @prefix bqbiol: <http://biomodels.net/biology-qualifiers/>.
#^ @prefix biopax: <http://www.biopax.org/release/biopax-level3.owl#>.
#^ @prefix prov: <http://www.w3.org/ns/prov#> .
#^ @prefix obo: <http://purl.obolibrary.org/obo/>.

#^ :model a rbmo:Kappa  ;
#^   dct:title "RBS" ;
#^   dct:description "Ribosome Binding Site" ;
#^   dct:creator "Matteo Cavaliere", "Goksel Misirli", "William Waites" ; 
#^   prov:derivesFrom <https://github.com/rulebased/composition/tree/master/templates/generic_part.kappa> .

#^ :DNA a rbmo:Agent ;  
#^   rbmo:hasSite :DNA:part .
#^ :DNA:part a rbmo:Site ;
#^   rbmo:hasState :DNA:part:IDENTIFIER .
#^ :DNA:part:IDENTIFIER a rbmo:State ;
#^   dct:title "RBS" ;
#^   sbol:role so:SO:0000552 .
%agent: DNA(binding,us,ds,part~IDENTIFIER)

#^ :RNA a rbmo:Agent ; 
#^   rbmo:hasSite :RNA:part .
#^ :RNA:part a rbmo:Site ;
#^   rbmo:hasState :RNA:part:IDENTIFIER .
#^ :RNA:part:IDENTIFIER a rbmo:State ;
#^   sbol:role so:SO:0000552 .
%agent: RNA(ds,us,binding,deg,part~IDENTIFIER)

#RULE 7: Translation initiation
%var: 'ribosome_binding_rate' 0.00001
#^ :translation-initiation a rbmo:Rule ;
#^   obo:part_of go:GO:0006413 ;
#^   dct:title "Translational initiation" .
'translation_initiation' RNA(binding,type~IDENTIFIER), Ribosome(rna) -> RNA(binding!1, type~IDENTIFIER), Ribosome(rna!1) @ 'ribosome_binding_rate'
