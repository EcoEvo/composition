#^ :t a rbmc:Template  ;
#^   dct:title "RBS" ;
#^   dct:description "Ribosome Binding Site" ;
#^   dct:creator "Matteo Cavaliere", "Goksel Misirli", "William Waites" ; 
#^   rbmc:include rbmt:transcription_elongation.kappa ;
